package xyz.sethy.cd.listener;

import net.minecraft.client.Minecraft;
import net.minecraftforge.event.Event.Result;
import net.minecraftforge.event.ForgeSubscribe;
import net.minecraftforge.event.entity.player.PlayerInteractEvent;

public class ItemUseListener {
	
	@ForgeSubscribe
	public void onItemUse(final PlayerInteractEvent event) {
		if (event.useItem.equals(Result.ALLOW)) {
			Minecraft.getMinecraft().getLogAgent().logInfo("Has eaten item");
		}
	}
}